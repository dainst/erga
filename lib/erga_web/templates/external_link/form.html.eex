<%= form_for @changeset, @action, [multipart: true], fn f ->%>
<%= if @changeset.action do %>
    <div class="alert alert-danger">
      <p>Oops, something went wrong! Please check the errors below.</p>
    </div>
  <% end %>

<%= hidden_input f, :project_id %>

<%= label f, :label %>
<%= text_input f, :label %>
<%= error_tag f, :label %>

<%= label f, :url %>
<%= text_input f, :url %>
<%= error_tag f, :url %>


<div>
  <%= submit "Save" %>
</div>

<% end %>
